QPushButton:hover{
        border-style: solid;
        border-width: 2px;
        color: rgb(0,0,0);
        padding: 2px;
        background-color: rgb(233,233,233);
}
QPushButton:pressed{
        border-style: solid;
        border-width: 2px;
        color: rgb(0,0,0);
        padding: 2px;
        background-color: rgb(200,200,200);
}
QPushButton:checked{
        border-style: solid;
        border-width: 2px;
        color: rgb(0,0,0);
        padding: 2px;
        background-color: rgb(200,200,200);
